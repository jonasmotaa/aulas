2) MRU 

#include <stdio.h>

int main(int argc, char const *argv[]) {
float d, v, t, vd, vv, vt;
printf("digite a distancia: \n");
scanf("%f", &d);

printf("digite a velocidade: \n");
scanf("%f", &v);

printf("digite o tempo: \n");
scanf("%f", &t);

//processamento

if(d == 0){
vd = v * t;
printf("A distancia eh: %f", vd);
}else if ( v == 0 ){
vv = d / t;
printf("A velocidade eh: %f\n", vv);

}else if (t == 0){
vt = d / v;
printf("o tempo eh: %f", vt);

}else{
printf("impossivel calcular.\n");
}
return 0;

}


