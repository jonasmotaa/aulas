#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(){
    int vetor[5];
    int i, x, total;
    total = 0;
    float media;
    for (i = 0; i < 5; i++){
        printf("digite o falor de %d \n", i);
            scanf("%d", &vetor[i]);
            total += vetor[i];
    }
media = total / 5;
    for (i = 0; i < 5; i++){
  printf("[%d] = %d\n", i, vetor[i]);

}

printf("Media = %.2f \n", media);
for (i = 0; i < 5; i++){
        if(vetor[i] >= media){
                printf("[%d] = %d\n", i, vetor[i]);
        }


    }

return 0;
}
